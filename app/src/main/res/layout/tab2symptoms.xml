<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.louis.prototype.MyDiary$PlaceholderFragment">

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter the severity of your symptoms below:"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/section_label"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="69dp"
        android:paddingTop="1dp"
        android:text="Acne" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/seekBar2"
        android:paddingTop="20dp"
        android:text="bloating" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:paddingTop="20dp"
        android:text="cramps" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:paddingTop="20dp"
        android:text="Dizziness" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:paddingTop="20dp"
        android:text="spots" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView11"
        android:paddingTop="20dp"
        android:text="Headaches" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12"
        android:layout_below="@+id/textView12"
        android:paddingTop="20dp"
        android:text="insomnia" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:layout_below="@+id/textView13"
        android:paddingTop="20dp"
        android:text="Sweating" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14"
        android:layout_below="@+id/seekBar5"
        android:layout_marginTop="14dp"
        android:backgroundTint="@color/colorPrimary"
        android:text="Submit" />

    <SeekBar
        android:id="@+id/seekBar5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar13"
        android:layout_alignStart="@+id/seekBar13"
        android:layout_below="@+id/seekBar13"
        android:minWidth="100dp"
        android:paddingTop="20dp" />

    <SeekBar
        android:id="@+id/seekBar13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar12"
        android:layout_alignStart="@+id/seekBar12"
        android:layout_below="@+id/seekBar12"
        android:minWidth="100dp"
        android:paddingTop="20dp" />

    <SeekBar
        android:id="@+id/seekBar12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar11"
        android:layout_alignStart="@+id/seekBar11"
        android:layout_below="@+id/seekBar11"
        android:minWidth="100dp"
        android:paddingTop="20dp" />

    <SeekBar
        android:id="@+id/seekBar11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar10"
        android:layout_alignStart="@+id/seekBar10"
        android:layout_below="@+id/seekBar10"
        android:minWidth="100dp"
        android:paddingTop="20dp" />

    <SeekBar
        android:id="@+id/seekBar10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar9"
        android:layout_alignStart="@+id/seekBar9"
        android:layout_below="@+id/seekBar9"
        android:minWidth="100dp"
        android:paddingTop="20dp" />

    <SeekBar
        android:id="@+id/seekBar9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar3"
        android:layout_alignStart="@+id/seekBar3"
        android:layout_below="@+id/seekBar3"
        android:minWidth="100dp"
        android:paddingTop="20dp" />

    <SeekBar
        android:id="@+id/seekBar3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar2"
        android:layout_alignStart="@+id/seekBar2"
        android:layout_below="@+id/seekBar2"
        android:minWidth="100dp"
        android:paddingTop="20dp" />

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView7"
        android:layout_marginEnd="53dp"
        android:layout_marginRight="53dp"
        android:minWidth="100dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView13"
        android:layout_alignRight="@+id/textView13"
        android:layout_below="@+id/section_label"
        android:layout_marginTop="22dp"
        android:text="Symptom"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/section_label"
        android:layout_alignRight="@+id/section_label"
        android:layout_alignTop="@+id/textView15"
        android:text="Severity 0-10"
        android:textStyle="bold" />

</RelativeLayout>
